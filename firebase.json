{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": [
    {
      "bucket": "cinechill-dev.appspot.com",
      "rules": "storage.rules"
    }
  ],
  "hosting": {
    "public": "build/web",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "headers": [
      {
        "source": "/assets/videos/**",
        "headers": [
          {
            "key": "Accept-Ranges",
            "value": "bytes"
          },
          {
            "key": "Content-Type",
            "value": "video/mp4"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "flutter": {
    "platforms": {
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "cinechill-dev",
          "configurations": {
            "web": "1:530294840005:web:2cdf8fa1f767e106dbaeac"
          }
        }
      }
    }
  }
}
